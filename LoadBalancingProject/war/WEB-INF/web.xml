<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoadBalancingProject</servlet-name>
		<servlet-class>com.loadbalancing.LoadBalancingProjectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoadBalancingProject</servlet-name>
		<url-pattern>/loadbalancingproject</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>loginValidate</servlet-name>
		<servlet-class>com.loadbalancing.loginValidate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginValidate</servlet-name>
		<url-pattern>/loginValidate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.loadbalancing.logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
